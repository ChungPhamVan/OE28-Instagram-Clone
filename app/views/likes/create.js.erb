<% if @new_like.likeable_type == Post.name %>
  $('#post-<%= @object.id %> #bookmark_like__like')
  .html('<%= j render("likes/unlike", object: @object) %>');
  $('#post-<%= @object.id %> #count_like_post button')
  .html('<%= j t(".count_like",
      count: count_like(@object)) %>');
<% else %>
  $('#comment-<%= @object.id %> #like-unlike-comment')
  .html('<%= j render("likes/unlike", object: @object) %>')
  $('#count-like-comment-<%= @object.id %>')
  .html('<%= j t(".count_like",
      count: count_like(@object)) %>')
<% end %>
