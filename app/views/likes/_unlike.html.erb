<%= form_for @post, url: like_path(@post),
  html: {method: :delete, class: "form_for_like" }, remote: true do |f| %>
  <%= hidden_field_tag :post_id, @post.id %>
  <%= hidden_field_tag :type_action, Settings.bookmark_like.like %>
  <%= button_tag class: "instagram-card-icon button_red" do %>
    <i class="fa fa-heart text-danger transform-scale"></i>
  <% end %>
<% end %>
